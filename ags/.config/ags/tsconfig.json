{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": [
      "ES2022"
    ],
    "allowJs": true,
    "checkJs": false,
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": "~/.config/ags",
    "typeRoots": [
      "./types/ags.d.ts",
      "./node_modules/@girs",
      "./types"
    ],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}